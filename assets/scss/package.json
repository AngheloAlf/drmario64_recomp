    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-scss": "^1.0.0-security",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-scss": "^5.3.1"
  },
  "dependencies": {
    "sass": "^1.75.0"
  }
}
